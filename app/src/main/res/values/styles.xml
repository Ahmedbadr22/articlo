<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Text -->
    <style name="TextAppearance.Material3.HeadlineSmall.Header" parent="Base.Widget.Material3.CardView">
        <item name="android:textSize">@dimen/_20sdp</item>
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="TextAppearance.Material3.TitleLarge.AppLogo" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textSize">@dimen/_30sdp</item>
    </style>

    <style name="Base.Widget.Material3.CardView.SelectedDot" parent="Base.Widget.Material3.CardView">
        <item name="android:layout_width">@dimen/_10sdp</item>
        <item name="android:layout_height">@dimen/_10sdp</item>
        <item name="cardBackgroundColor">?attr/colorPrimary</item>
        <item name="cardCornerRadius">150dp</item>
    </style>

    <style name="Base.Widget.Material3.CardView.UnSelectedDot" parent="Base.Widget.Material3.CardView">
        <item name="android:layout_width">@dimen/_10sdp</item>
        <item name="android:layout_height">@dimen/_10sdp</item>
        <item name="cardBackgroundColor">@color/gray</item>
        <item name="cardCornerRadius">150dp</item>
    </style>



</resources>